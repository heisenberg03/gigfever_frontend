{
    "expo": {
      "name": "GigFever",
      "slug": "gigfever",
      "version": "1.0.0",
      "orientation": "portrait",
      "icon": "./src/assets/icon.png",
      "splash": {
        "image": "./src/assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "updates": {
        "fallbackToCacheTimeout": 0
      },
      "assetBundlePatterns": ["**/*"],
      "ios": {
        "supportsTablet": true
      },
      "android": {
        "adaptiveIcon": {
          "foregroundImage": "./assets/adaptive-icon.png",
          "backgroundColor": "#ffffff"
        }
      },
      "web": {
        "favicon": "./src/assets/favicon.png"
      },
      "newArchEnabled": true
    }
  }